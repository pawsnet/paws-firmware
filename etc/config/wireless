config wifi-device  radio0
	option type     mac80211
	option channel  auto
	option hwmode	11ng
	option htmode	HT20
	list ht_capab	SHORT-GI-40
	list ht_capab	TX-STBC
	list ht_capab	RX-STBC1
	list ht_capab	DSSS_CCK-40



config wifi-iface
	option device   radio0
	option network  paws
	option mode     ap
	option ssid     PAWS
	option encryption psk2
	option key bismark123


config wifi-device  radio1
	option type     mac80211
	option channel  36
	option hwmode	11na
	option htmode	HT20
	list ht_capab	SHORT-GI-40
	list ht_capab	TX-STBC
	list ht_capab	RX-STBC1
	list ht_capab	DSSS_CCK-40
	# REMOVE THIS LINE TO ENABLE WIFI:
	option disabled 1 

config wifi-iface
	option device   radio1
	option network  lan
	option mode     ap
	option ssid     BISmark5
	option encryption psk2
	option key bismark123

